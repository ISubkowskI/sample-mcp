{
  "Serilog": {
    "MinimumLevel": "Verbose",
    "WriteTo": [
      { "Name": "Debug" },
      {
        "Name": "Console",
        "Args": {
          "standardErrorFromLevel": "Warning"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ]
  },
  "IdentityStorageApi": {
    "ApiUrl": "http://localhost:5023",
    "ApiBasePath": "/api/v1"
  },
  "App": {
    "Name": "Claims MCP Server 'Stdio'",
    "Version": "1.1.3"
  }
}